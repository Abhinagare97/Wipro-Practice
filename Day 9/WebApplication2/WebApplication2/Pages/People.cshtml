@page
@model WebApplication2.Pages.PeopleModel
@{
	<h1>People</h1>
	<table class="table">
		<thead>
		<tr>
			<th>ID</th>
			<th>NAME</th>
			<th>DESCRIPTION</th>
		<tr>
		</thead >
		<tbody>
			@foreach (var a  in Model.people)
			{
				<tr>
				<td>@a.Id</td>
				<td>@a.Name</td>
				<td>@a.Description</td>
				</tr>
			}
		</tbody>
		
	</table>
}
